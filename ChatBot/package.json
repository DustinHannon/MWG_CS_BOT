{
  "name": "mwg-cs-bot",
  "version": "1.0.0",
  "description": "Morgan White Group Customer Service ChatBot with enhanced features and usability. Prerequisites: Node.js >= 16.0.0 must be installed from https://nodejs.org/",
  "main": "server/main.js",
  "type": "module",
  "scripts": {
    "start": "node server/main.js",
    "dev": "nodemon server/main.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "security-audit": "npm audit fix --force",
    "update-deps": "npm update --save/--save-dev && npm audit fix --force"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "dompurify": "^3.0.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.0",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "highlight.js": "^11.8.0",
    "marked": "^9.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "overrides": {
    "glob": "^10.3.3",
    "inflight": "npm:lru-cache@^10.0.1",
    "semver": "^7.5.4",
    "word-wrap": "^1.2.4",
    "tough-cookie": "^4.1.3",
    "@babel/traverse": "^7.23.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MorganWhiteGroup/MWG_CS_BOT.git"
  },
  "keywords": [
    "chatbot",
    "customer-service",
    "morgan-white-group",
    "openai",
    "express",
    "nodejs"
  ],
  "author": "Morgan White Group",
  "license": "MIT",
  "private": true
}
